{
    "javascript": {
        "eval": [
            "nilstho-sipschedulerservice.js"
        ]
    },
    "apis": {
        "nilstho-sipscheduler": {
            "hidden": true
        }
    },
    "database": {
        "init": [{
            "cmd": "table",
            "name": "sip_scheduler_state",
            "init": [{
                    "cmd": "column",
                    "name": "id",
                    "type": "INTEGER PRIMARY KEY"
                },
                {
                    "cmd": "column",
                    "name": "active",
                    "type": "BOOLEAN"
                },
                {
                    "cmd": "column",
                    "name": "mac",
                    "type": "TEXT"
                },
                {
                    "cmd": "column",
                    "name": "sip_list",
                    "type": "TEXT"
                },
                {
                    "cmd": "column",
                    "name": "prev_disabled",
                    "type": "TEXT"
                },
                {
                    "cmd": "column",
                    "name": "until_ms",
                    "type": "BIGINT"
                }
            ]
        }]
    },
    "config": {
        "init": [{
                "cmd": "item",
                "name": "pbxmacaddress",
                "type": "STRING",
                "default": "009033FFFE00",
                "password": false
            },
            {
                "cmd": "item",
                "name": "domain",
                "type": "STRING",
                "default": "",
                "password": false
            },
            {
                "cmd": "item",
                "name": "pbxname",
                "type": "STRING",
                "default": "master",
                "password": false
            },
            {
                "cmd": "item",
                "name": "run_at",
                "type": "STRING",
                "default": "00:00",
                "password": false
            },
            {
                "cmd": "item",
                "name": "enabled",
                "type": "BOOL",
                "default": true
            },
            {
                "cmd": "item",
                "name": "sip_mask",
                "type": "INT",
                "default": 1
            },
            {
                "cmd": "item",
                "name": "re_register",
                "type": "INT",
                "default": 5
            },
            {
                "cmd": "item",
                "name": "failover_enabled",
                "type": "BOOL",
                "default": false
            },
            {
                "cmd": "item",
                "name": "standby_pbxmacaddress",
                "type": "STRING"
            },
            {
                "cmd": "item",
                "name": "failover_delay_ms",
                "type": "INT",
                "default": 60000
            },
            {
                "cmd": "item",
                "name": "failover_poll_ms",
                "type": "INT",
                "default": 5000
            },
            {
                "cmd": "item",
                "name": "failover_confirm_polls",
                "type": "INT",
                "default": 2
            },
            {
                "cmd": "item",
                "name": "failback_delay_ms",
                "type": "INT",
                "default": 15000
            },
            {
                "cmd": "item",
                "name": "failback_confirm_polls",
                "type": "INT",
                "default": 2
            },
            {
                "cmd": "item",
                "name": "failover_master_ip",
                "type": "STRING",
                "default": ""
            },
            {
                "cmd": "item",
                "name": "failover_master_dns",
                "type": "STRING",
                "default": ""
            },
            {
                "cmd": "item",
                "name": "failover_standby_ip",
                "type": "STRING",
                "default": ""
            },
            {
                "cmd": "item",
                "name": "failover_standby_dns",
                "type": "STRING",
                "default": ""
            },
            {
                "cmd": "mode",
                "name": "pbx-manager",
                "read": true,
                "write": true
            }
        ]
    }
}